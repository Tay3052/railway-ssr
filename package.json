    "react": "^18.3.1",
    "react-comment": "^2.6.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0",
    "sass": "^1.76.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.8",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
